.headline {
  font-family: Helvetica, sans-serif;
  color: #828282;
  font-size: 24px;
}
/* ---- text and links ---- */
.sitenameSubtitle {
  font-family: Helvetica, sans-serif;
  font-size: 16px;
  color: #8F8F8F;
  text-transform: none;
}
.bodytext-10pt {
  color: #7E7E7E;
  font-size: 10pt;
}
.bodysmall {
  font-size: 8pt;
  color: #999999;
}
.bodysmallgrey {
  font-size: 8pt;
  color: #828282;
}
.Subhead-15px-Gray {
  color: #4f4f4f;
  font-size: 15px;
  font-weight: bold;
  border-bottom: 1px solid #A1A19F;
  margin: 0;
  padding: 0 5px 0;
}
body {
  padding: 0;
  margin: 0;
}
.su_bootstrap_safe a {
  font-family: Helvetica, sans-serif;
  color: #4f4f4f;
}
.su_bootstrap_safe a:hover {
  color: #828282;
}
.su_bootstrap_safe h1,
.su_bootstrap_safe h2,
.su_bootstrap_safe h3,
.su_bootstrap_safe h4,
.su_bootstrap_safe h5,
.su_bootstrap_safe h6 {
  color: #363636;
}
.su_bootstrap_safe p {
  line-height: 1.5;
  font-weight: 300;
}
.su_bootstrap_safe #body {
  font-weight: 300;
  overflow: hidden;
  border-top-width: 7px;
  border-top-style: solid;
  border-top-color: #4f4f4f;
  position: relative;
  font-family: Helvetica, sans-serif;
  color: #7E7E7E;
  font-size: 16px;
  line-height: 1.2;
}
.su_bootstrap_safe header {
  padding-top: 17px;
  padding-bottom: 10px;
}
.su_bootstrap_safe #sitename h1 {
  margin: 0;
  font-size: 1.875rem;
  text-transform: uppercase;
  font-weight: 300;
}
.su_bootstrap_safe #sitename h2 {
  font-size: 1rem;
  font-weight: 300;
  color: #828282;
  margin-top: 4px;
}
.su_bootstrap_safe #banner {
  background: url("/images/banner.jpg") no-repeat top left;
  background-size: cover;
  background-color: #4f4f4f;
}
.su_bootstrap_safe #banner-image {
  background: url("/images/flame.jpg") no-repeat top right;
  background-size: contain;
  background-position: right 17% top;
  height: 255px;
  z-index: 0;
  overflow: hidden;
}
.su_bootstrap_safe #banner-image.su {
  background: none;
}
.su_bootstrap_safe #sdaLogo {
  background-image: url("/images/logo.gif");
  background-repeat: no-repeat;
  background-position: top right;
  background-size: contain;
  height: 43px;
}
.su_bootstrap_safe #sitename {
  font-size: 1.5rem;
  text-transform: uppercase;
}
.su_bootstrap_safe #search.search_field {
  text-align: left;
  width: 153px;
  height: 23px;
  border: 1px solid #A2A2A2;
  color: #8B8B8B;
  margin: 0 9px 0 0;
  padding: 3px;
}
.su_bootstrap_safe #search > a.circle {
  padding: 4px 9px;
  float: left;
  color: rgba(79, 79, 79, 0.3);
}
.su_bootstrap_safe #search form {
  float: left;
}
.su_bootstrap_safe #navigation {
  background-color: rgba(79, 79, 79, 0.65);
}
.su_bootstrap_safe #navigation-container {
  padding: 6px 0;
}
.su_bootstrap_safe #navigation-container #navigation-span {
  text-align: center;
  min-height: 17px;
}
.su_bootstrap_safe #navigation-container #navigation-span ul {
  display: inline;
}
.su_bootstrap_safe #navigation-container #navigation-span li {
  list-style: none;
  display: inline-block;
  color: #FFFFFF;
  font-size: 15px;
  margin: 0px;
  border: 0px;
  padding: 0px;
}
.su_bootstrap_safe #navigation-container #navigation-span li.navigation_separator {
  display: none;
}
.su_bootstrap_safe #navigation-container #navigation-span a {
  color: #FFFFFF;
  padding: 5px 7px;
  text-decoration: none;
}
.su_bootstrap_safe #navigation-container #navigation-span a:hover {
  color: #FFFFFF;
  text-decoration: underline;
  background-color: transparent;
}
.su_bootstrap_safe .headline {
  font-family: Helvetica, sans-serif;
  color: #4f4f4f;
  font-size: 24px;
}
#sidebar > table > tbody > tr > td > a > img {
	padding: 5px;
}
.su_bootstrap_safe .FeedDescription {
	font-family: Helvetica, sans-serif;
	color: #7E7E7E;
	font-size: 16px;
	line-height: 1.2;
}
.su_bootstrap_safe .FeedTitle {
	font-family: Helvetica, sans-serif;
	color: #7E7E7E;
	font-size: 16px;
	line-height: 1.2;
	font-weight: bold;
}
.su_bootstrap_safe article {
  padding-top: 11px;
  padding-bottom: 40px;
}
.su_bootstrap_safe #livestream {
  width: 620px;
  height: 465px;
  display: block;
  margin: 0 auto;
}
.su_bootstrap_safe #body.article {
  border: none;
  text-align: left;
  background-color: transparent;
  margin: 15px 0;
  padding: 10px 14px 21px;
}
.su_bootstrap_safe #content-left-span {
  padding-top: 15px;
}
.su_bootstrap_safe #body.article #content-span1 {
  padding-top: 15px;
  padding-bottom: 15px;
}
.su_bootstrap_safe #sidebar {
  padding-top: 11px;
  padding-right: 30px;
  padding-bottom: 40px;
  padding-left: 20px;
  background-color: rgba(79, 79, 79, 0.1);
  background: -webkit-gradient(linear, left top, left bottom, from(rgba(79, 79, 79, 0.1)), to(#ffffff));
  background: -webkit-linear-gradient(top, rgba(79, 79, 79, 0.1), #ffffff);
  background: -moz-linear-gradient(top, rgba(79, 79, 79, 0.1), #ffffff);
  background: -ms-linear-gradient(top, rgba(79, 79, 79, 0.1), #ffffff);
  background: -o-linear-gradient(top, rgba(79, 79, 79, 0.1), #ffffff);
}
.su_bootstrap_safe #footer {
  background-color: rgba(79, 79, 79, 0.1);
  background: -webkit-gradient(linear, left top, left bottom, from(rgba(79, 79, 79, 0.1)), to(#ffffff));
  background: -webkit-linear-gradient(top, rgba(79, 79, 79, 0.1), #ffffff);
  background: -moz-linear-gradient(top, rgba(79, 79, 79, 0.1), #ffffff);
  background: -ms-linear-gradient(top, rgba(79, 79, 79, 0.1), #ffffff);
  background: -o-linear-gradient(top, rgba(79, 79, 79, 0.1), #ffffff);
  font-size: 8pt;
  /* text-align: center; */
  color: #4F4F4F;
  /* height: 77px; */
  height: 150px;
  padding: 10px;
  line-height: 1.4;
}
.su_bootstrap_safe #footer .footer .bottom_links {
  font-size: inherit;
  text-decoration: none;
}
.su_bootstrap_safe #footer .footer a.bottom_links:hover {
  text-decoration: underline;
}
/* ---- Responsive ---- */
/* Large desktop */
@media (min-width: 1200px) {
  #banner-logo {
    margin-left: -133px;
  }
  #livestream {
    width: 770px;
    height: 578px;
    display: block;
    margin: 0 auto;
  }
}
/* Portrait tablet to landscape and desktop */
@media (min-width: 768px) and (max-width: 979px) {
  #banner-logo {
    width: 355px;
  }
  #livestream {
    width: 476px;
    height: 357px;
    display: block;
    margin: 0 auto;
  }
}
/* Landscape phone to portrait tablet */
@media (max-width: 767px) {
  #sdaLogo {
    background-position: top left;
  }
  header {
    padding-left: 5px;
    padding-right: 5px;
  }
  #content {
    padding-left: 5px;
    padding-right: 5px;
  }
  #banner-image {
    height: 200px;
  }
  #sitename {
    float: left;
  }
  #banner-image {
    background-size: cover;
  }
  #livestream {
    width: 476px;
    height: 357px;
    display: block;
    margin: 0 auto;
  }
}
/* Landscape phones and down */
@media (max-width: 480px) {
  #sdaLogo {
    background-position: top left;
  }
  #livestream {
    width: 305px;
    height: 229px;
    display: block;
    margin: 0 auto;
  }
}
